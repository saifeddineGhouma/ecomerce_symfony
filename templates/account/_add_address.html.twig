{% extends 'base.html.twig' %}

{% block title %} my account!{% endblock %}

{% block contenu %}
<h1>Add Adress</h1>
<hr> 
{{ form_start(form)}}

<div class="row mb-5">
    <div class="col-md-4">
        {{ form_row(form.name)}} 
    </div>
    <div class="col-md-4">
        {{ form_row(form.firstname)}} 
    </div>
    <div class="col-md-4">
        {{ form_row(form.lastname)}} 
    </div>

    <div class="col-md-6">
        {{ form_row(form.adress)}} 
    </div>
    <div class="col-md-6">
        {{ form_row(form.postal)}} 
    </div>
    <div class="col-md-6">
        {{ form_row(form.city)}} 
    </div>
    <div class="col-md-6">
        {{ form_row(form.country)}} 
    </div>
    <div class="col-md-4">
        {{ form_row(form.phone)}} 
    </div>
    <div class="col-md-4">
        {{ form_row(form.company)}} 
    </div>
    <div class="col-md-12">
        {{ form_row(form.Save)}} 
    </div>
</div>

{{form_end(form)}}



{% endblock %}
